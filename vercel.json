{
    "version": 2,
    "builds": [
        {
          "src": "src/main.ts",
          "use": "@vercel/node"
        }
  ],
  "routes": [
    {
      "src": "src/(.*)",
      "dest": "/",
      "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
         ]
      }
    ]
  }